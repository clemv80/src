<?php

namespace BlogBundle\Entity;

/**
 * SignalementArt
 */
class SignalementArt
{
    /**
     * @var int
     */
    private $id;

    /**
     * @var \DateTime
     */
    private $creationdt;


    /**
     * Get id
     *
     * @return int
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set creationdt
     *
     * @param \DateTime $creationdt
     *
     * @return SignalementArt
     */
    public function setCreationdt($creationdt)
    {
        $this->creationdt = $creationdt;

        return $this;
    }

    /**
     * Get creationdt
     *
     * @return \DateTime
     */
    public function getCreationdt()
    {
        return $this->creationdt;
    }
    /**
     * @var \BlogBundle\Entity\User
     */
    private $user;


    /**
     * Set user
     *
     * @param \BlogBundle\Entity\User $user
     *
     * @return SignalementArt
     */
    public function setUser(\BlogBundle\Entity\User $user = null)
    {
        $this->user = $user;

        return $this;
    }

    /**
     * Get user
     *
     * @return \BlogBundle\Entity\User
     */
    public function getUser()
    {
        return $this->user;
    }
    /**
     * @var \BlogBundle\Entity\Article
     */
    private $article;


    /**
     * Set article
     *
     * @param \BlogBundle\Entity\Article $article
     *
     * @return SignalementArt
     */
    public function setArticle(\BlogBundle\Entity\Article $article = null)
    {
        $this->article = $article;

        return $this;
    }

    /**
     * Get article
     *
     * @return \BlogBundle\Entity\Article
     */
    public function getArticle()
    {
        return $this->article;
    }
}
